{
    "$schema": "../gen/schemas/desktop-schema.json",
    "identifier": "dialog",
    "description": "Capability for the dialog window",
    "windows": [
        "dialog"
    ],
    "permissions": [
        "core:default",
        "core:window:allow-hide",
        "core:window:allow-start-dragging",
        "core:app:allow-default-window-icon",
        "core:window:allow-set-title"
    ]
}